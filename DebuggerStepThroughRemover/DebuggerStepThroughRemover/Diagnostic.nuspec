<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>DebuggerStepThroughRemover</id>
    <version>1.0.0.0</version>
    <title>DebuggerStepThroughRemover</title>
    <authors>Thijs Brobbel</authors>
    <owners>Thijs Brobbel</owners>
    <licenseUrl>https://github.com/tiesmaster/DebuggerStepThroughRemover/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/tiesmaster/DebuggerStepThroughRemover</projectUrl>
    <iconUrl>https://www.google.nl/search?q=test+icon</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
        This is just a test diagnostic extension to see if I can
        remove the debugger step through attribute using a code
        fixer.
    </description>
    <copyright>Copyright</copyright>
    <tags>DebuggerStepThroughRemover, analyzers</tags>
  </metadata>
  <!-- The convention for analyzers is to put language agnostic dlls in analyzers\dotnet and language specific analyzers in either analyzers\dotnet\cs or analyzers\dotnet\vb -->
  <files>
    <file src="*.dll" target="analyzers\dotnet\cs" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*;**\System.Composition.*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>
